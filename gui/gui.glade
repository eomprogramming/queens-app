<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkListStore" id="eventdata">
    <columns>
      <!-- column-name name -->
      <column type="gchararray"/>
      <!-- column-name location -->
      <column type="gchararray"/>
      <!-- column-name description -->
      <column type="gchararray"/>
      <!-- column-name imglocation -->
      <column type="gchararray"/>
      <!-- column-name image -->
      <column type="GdkPixbuf"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">Speech by Cthulhu</col>
        <col id="1" translatable="yes">R'lyeh Conference Centre</col>
        <col id="2" translatable="yes">I&#xE4;, i&#xE4;, Cthulhu f'tagn!</col>
        <col id="3" translatable="yes">cthulhu_head.png</col>
        <col id="4"/>
      </row>
    </data>
  </object>
  <object class="GtkListStore" id="sponsorlist">
    <columns>
      <!-- column-name value -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">Hastur</col>
      </row>
      <row>
        <col id="0" translatable="yes">Azathoth</col>
